{
  "deno.enable": true,
  "deno.importMap": "./import_map.json",
  "deno.lint": true,
  "deno.unstable": false,
  "deno.path": "/usr/local/bin/deno",
  "deno.config": "./deno.json",
  "[json]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "editor.formatOnSave": true,
  "editor.tabSize": 2
}
